              Scores &amp;
              %br.menu-break/
              Schedule
              %span.caret
            %ul.dropdown-menu.dropdown-menu-fixed
              %li
                %a{href:schedules_path(season: Season.current.id, league: 3)} Summer Weekdays
              %li{:style => "border-bottom: 1px solid #fff!important;"}
