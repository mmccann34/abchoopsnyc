                  Past
                  %br/
                  Seasons
          %li.dropdown-fixed.dropdown-fixed-stats
            %a.dropdown-toggle.dropdown-toggle-stats{"data-toggle" => "dropdown", :href => results_url}
              Stats &amp;
              %br.menu-break/
              Standings
              %span.caret
            %ul.dropdown-menu.dropdown-menu-fixed
              %li
                %a{href:results_url(season: Season.current.id, league: 3)} Summer Weekdays
              %li{:style => "border-bottom: 1px solid #fff!important;"}
