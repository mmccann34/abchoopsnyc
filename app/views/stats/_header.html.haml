.navbar
  .container-fluid.container-fluid-header
    .navbar-header{:style => "width:100%"}
      %button.navbar-toggle.collapsed{"data-target" => "#myNavbar", "data-toggle" => "collapse", :type => "button"}
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      #color_logo.header-aside
        %a{:href => "http://abchoops.com"}
          =image_tag 'whitelogo.png', class: 'header-logo'
      #myNavbar.collapse.navbar-collapse
        .header_slide_logo_container{:style => "border:none"}
          %a{:href => "http://abchoops.com"}
            =image_tag 'whitelogo.png', class: 'header-logo white-header-logo'
        %ul.nav.navbar-nav
          %li.dropdown-fixed.dropdown-fixed-theleague
            %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "http://abchoops.com/theleague.html"}
              The
              %br.menu-break/
              League
              %span.caret
            %ul.dropdown-menu.dropdown-menu-fixed.dropdown-menu-theleague
              %li
